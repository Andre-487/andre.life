{{#extend "layout" page="wishList" lang="ru" headerNoMargin=true}}
    {{#content "description"}}A wish list of Andrey Prokopyuk{{/content}}

    {{#content "content"}}
        <section class="document-section">
            <ul>
                {{#each wishList}}
                    <li>
                        <a href="{{this.link}}" target="_blank" rel="noopener">
                            {{this.title}}
                        </a>
                    </li>
                {{/each}}
            </ul>
        </section>
    {{/content}}
{{/extend}}
